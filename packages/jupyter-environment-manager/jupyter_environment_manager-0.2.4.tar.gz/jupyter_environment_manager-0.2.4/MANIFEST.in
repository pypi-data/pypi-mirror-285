include LICENSE
include *.md
include pyproject.toml
recursive-include jupyter-config *.json
recursive-include tools *.py *.sh

include package.json
include install.json
include ts*.json
include yarn.lock
include environment.yml

graft jupyter_environment_manager/labextension

# Javascript files
graft src
graft style
prune **/node_modules
prune lib
prune binder

# Patterns to exclude from any directory
global-exclude *~
global-exclude *.pyc
global-exclude *.pyo
global-exclude .git
global-exclude .ipynb_checkpoints

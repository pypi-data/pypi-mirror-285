{
  "name": "@qbraid/jupyter-environment-manager",
  "private": true,
  "version": "0.2.4",
  "description": "JupyterLab extension for managing execution environments, packages, and kernels.",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/qBraid/qBraid-Lab",
  "bugs": {
    "url": "https://github.com/qBraid/qBraid-Lab/issues"
  },
  "author": {
    "name": "qBraid Development Team",
    "email": "contact@qbraid.com"
  },
  "license": "Proprietary",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "repository": {
    "type": "git",
    "url": "https://github.com/qBraid/lab-environment-manager.git"
  },
  "scripts": {
    "build": "jlpm run build:lib && jlpm run build:labextension:dev",
    "build:prod": "jlpm run clean && jlpm run build:lib && jlpm run build:labextension",
    "build:labextension": "jupyter labextension build .",
    "build:labextension:dev": "jupyter labextension build --development True .",
    "build:lib": "tsc",
    "clean": "jlpm run clean:lib",
    "clean:lib": "rimraf lib tsconfig.tsbuildinfo",
    "clean:labextension": "rimraf jupyter_environment_manager/labextension",
    "clean:all": "jlpm run clean:lib && jlpm run clean:labextension",
    "eslint": "eslint . --ext .ts,.tsx --fix",
    "eslint:check": "eslint . --ext .ts,.tsx",
    "prettier": "prettier --write \"**/*{.ts,.tsx,.js,.jsx,.css,.json,.md}\"",
    "install:extension": "jlpm run build",
    "watch": "run-p watch:src watch:labextension",
    "watch:src": "tsc -w",
    "watch:labextension": "jupyter labextension watch ."
  },
  "resolutions": {
    "@types/node": "^14.6.1",
    "react": "^17.0.1"
  },
  "dependencies": {
    "@aws-amplify/auth": "^4.3.11",
    "@aws-amplify/cli": "^6.3.1",
    "@aws-amplify/storage": "^5.0.8",
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@jupyterlab/application": "^3.1.0",
    "@jupyterlab/apputils": "^3.1.0",
    "@jupyterlab/coreutils": "^5.1.0",
    "@jupyterlab/launcher": "^3.1.0",
    "@jupyterlab/services": "^6.1.0",
    "@jupyterlab/ui-components": "^3.5.3",
    "@mui/icons-material": "^5.8.4",
    "@mui/material": "^5.15.10",
    "alt": "^0.17.4",
    "react-ace": "^9.3.0",
    "superagent": "^5.1.0",
    "superagent-use": "^0.1.0"
  },
  "devDependencies": {
    "@jupyterlab/builder": "^3.1.0",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "mkdirp": "^1.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "typescript": "~4.1.3"
  },
  "sideEffects": [
    "style/*.css",
    "style/index.js"
  ],
  "styleModule": "style/index.js",
  "publishConfig": {
    "access": "public"
  },
  "jupyterlab": {
    "discovery": {
      "server": {
        "managers": [
          "pip"
        ],
        "base": {
          "name": "jupyter_environment_manager"
        }
      }
    },
    "extension": true,
    "outputDir": "jupyter_environment_manager/labextension",
    "_build": {
      "load": "static/remoteEntry.1e4da77baefa46ce6e60.js",
      "extension": "./extension",
      "style": "./style"
    }
  },
  "jupyter-releaser": {
    "hooks": {
      "before-build-npm": [
        "python -m pip install jupyterlab~=3.4",
        "jlpm"
      ]
    }
  }
}

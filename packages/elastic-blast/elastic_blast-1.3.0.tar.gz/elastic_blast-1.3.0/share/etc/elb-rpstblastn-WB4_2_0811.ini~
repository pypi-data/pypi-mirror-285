# ElasticBLAST configuration file to run rpstblastn

[cloud-provider]
gcp-project = ncbi-sandbox-blast
gcp-region = us-east4
gcp-zone = us-east4-b

[cluster]
num-nodes = 2
num-cpus = 30
pd-size = 3000G
use-preemptible = 1

[blast]
program = rpstblastn
options =
db = split-cdd
queries = gs://elastic-blast-samples/queries/WB4_2_0811/RFQT01.1.fsa_nt.gz


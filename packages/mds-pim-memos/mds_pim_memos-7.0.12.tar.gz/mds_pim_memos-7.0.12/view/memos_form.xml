<form col="6">
    <label name="name"/>
    <field name="name" colspan="3"/>
    <label name="sequence"/>
    <field name="sequence"/>
    <label name="category"/>
    <field name="category"/>
    <label name="datecreated"/>
    <field name="datecreated" widget="date"/>
    <label name="datechanged"/>
    <field name="datechanged" widget="date"/>
    <notebook colspan="6">
        <page string="Note" col="1" name="memo2">
            <field name="memo2" widget="richtext"/>
        </page>
        <page string="Children" col="1" id="childs">
            <field name="childs"/>
        </page>
    </notebook>
</form>

    "@smui/icon-button": "latest",
    "@smui/layout-grid": "latest",
    "@smui/menu-surface": "latest",
    "@smui/segmented-button": "latest",
    "@smui/slider": "latest",
    "@smui/tooltip": "latest",
    "@smui-extra/badge": "latest",
    "@sveltejs/adapter-auto": "latest",
    "@sveltejs/adapter-static": "latest",
    "@sveltejs/kit": "latest",
    "@tsconfig/svelte": "latest",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "eslint": "latest",
    "eslint-config-prettier": "latest",

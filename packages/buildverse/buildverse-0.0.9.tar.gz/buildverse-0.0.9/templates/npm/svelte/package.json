    "smui-theme": "latest",
    "svelte": "latest",
    "svelte-check": "latest",
    "svelte-kit": "latest",
    "svelte-mount": "latest",
    "svelte-preprocess": "latest",
    "tslib": "latest",
    "typescript": "latest",
    "vite": "latest"

<?xml version="1.0" encoding="utf-8" ?>
<odoo noupdate="0">
    <record id="storage_attachment_demo" model="dms.storage">
        <field name="name">Attachment Storage</field>
        <field name="save_type">attachment</field>
        <field name="inherit_access_from_parent_record" eval="True" />
        <field name="include_message_attachments" eval="True" />
        <field name="model_ids" eval="[(6, 0, [ref('base.model_res_partner')])]" />
    </record>
    <record id="insurance_data_directory" model="dms.directory">
        <field name="name">insurances</field>
        <field name="is_root_directory" eval="True" />
        <field name="parent_id" eval="False" />
        <field name="color" eval="2" />
        <field name="root_storage_id" ref="storage_attachment_demo" />
        <field name="category_id" ref="dms_res_model_root_directory.category_insurances_data" />
        <field name="model_id" ref="base.model_res_partner" />
        <field name="res_model">res.partner</field>
    </record>
    <record id="id_data_directory" model="dms.directory">
        <field name="name">ID data</field>
        <field name="is_root_directory" eval="True" />
        <field name="parent_id" eval="False" />
        <field name="color" eval="2" />
        <field name="root_storage_id" ref="storage_attachment_demo" />
        <field name="category_id" ref="dms_res_model_root_directory.category_id_data" />
        <field name="model_id" ref="base.model_res_partner" />
        <field name="res_model">res.partner</field>
    </record>
</odoo>